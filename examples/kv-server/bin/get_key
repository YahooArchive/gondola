#!/bin/sh

if [ $# != 1 ]; then
  echo "Get content from web service";
  echo "";
  echo "Usage: $0 key";
  exit;
fi

hostIds=( ["8080"]="host1" ["8081"]="host2" ["8082"]="host3")
ports="8080 8081 8082"
for port in $ports; do
  v=`curl -s localhost:$port/api/entries/$1`
  echo "${hostIds[port]}: $v"
done
